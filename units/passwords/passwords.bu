passwd:
  users:
    - name: passwords
      primary_group: passwords
      shell: /bin/false
      groups:
        - dockerhub
  groups:
    - name: passwords
systemd:
  units:
    - name: vaultwarden.service
      enabled: true
storage:
  directories:
    - path: /var/mnt/vault/warden
      mode: 0o700
      user:
        name: passwords
      group:
        name: passwords
