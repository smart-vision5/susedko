passwd:
  users:
    - name: status
      primary_group: status
      shell: /bin/false
      groups:
        - dockerhub
  groups:
    - name: status
systemd:
  units:
    - name: glances.service
      enabled: true
storage:
  files:
    - path: /usr/local/etc/nginx/conf.d/susedko.local.conf
      mode: 0o444
      user:
        name: domains
      group:
        name: domains

