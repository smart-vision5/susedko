passwd:
  users:
    - name: status
      uid: 1008
      primary_group: status
      no_create_home: true
      shell: /bin/false
      groups:
        - dockerhub
  groups:
    - name: status
      gid: 1008
systemd:
  units:
    - name: glances.service
      enabled: true
storage:
  files:
    - path: /var/lib/systemd/linger/status
      mode: 0644
    - path: /usr/local/etc/nginx/conf.d/susedko.local.conf
      mode: 0o444
