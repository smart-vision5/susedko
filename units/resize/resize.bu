systemd:
  units:
    - name: ffmpeg.service
      enabled: true
storage:
  files:
    - path: /home/ai/bin/resize
      mode: 0o555
      contents:
        inline: |
          #!/bin/bash
          input_video="$1"
          output_video="${input_video%.*} resized.${input_video##*.}"
          ffmpeg -i "$input_video" -vf "scale=1024:-1" "$output_video"
